{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "Weather Extension",
    "description": "This is a Chrome extension built with React & Tailwind By @saugatAdhikari",
    "action": {
        "default_popup": "js/index.html",
        "default_title": "Weather Extension"
    },
    "host_permissions": [
        "https://api.openweathermap.org/data/2.5/weather"
    ]
}